{
  "name": "fangcloud/fangcloud-php-sdk",
  "description": "Fangcloud SDK for PHP",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Fangcloud Open Platform"
    }
  ],
  "require": {
    "php": ">=5.5",
    "guzzlehttp/guzzle": "^6.2.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8.35|^5.4.0",
    "mockery/mockery": "@stable",
    "sami/sami": "@stable"
  },
  "autoload": {
    "psr-4": {
      "Fangcloud\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fangcloud\\Test\\": "tests/"
    }
  }
}